@model MtgCollectionMgr.Models.CollectionModel
@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<div class="row">
    <div class="col-md-3">
        <form asp-controller="CollectionModels" asp-action="Search">
            <div class="form-group">
                <label>Search for a Card</label>
                <input type="text" name="searchTerm" class="form-control" />
            </div>
            <input type="submit" value="Search" />
        </form>
    </div>
</div>

